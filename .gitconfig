[core]
  editor      = vim
[user]
  name        = Brendan ODonnell
  email       = brendan.james.odonnell@gmail.com
[github]
  user        = b0d0nne11
[push]
  default     = current
[pull]
  default     = current
  rebase      = true
[color]
  branch      = auto
  diff        = auto
  status      = auto
[color "branch"]
  current     = yellow reverse
  local       = yellow
  remote      = green
[color "diff"]
  meta        = yellow bold
  frag        = magenta bold
  old         = red bold
  new         = green bold
[color "status"]
  added       = yellow
  changed     = green
  untracked   = cyan
[help]
  autocorrect = 1
[alias]
  l           = pull --prune
  p           = push origin HEAD
  d           = diff --word-diff
  a           = add
  rm          = add -u
  c           = commit
  ca          = commit -a
  co          = checkout
  nb          = checkout -t -b
  s           = status -sb
  ll          = git log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
